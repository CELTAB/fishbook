extends base_layout

block append content
  div(class="container")
    div(class="row")
      div(class="col-md-6")
        form(role="form" method="post" action="import" enctype="multipart/form-data")
          div(class="form-group")

            label(for="importFile") File
            input(type="file" id="importFile" name="import_file" enctype="multipart/form-data")

          button(type="submit" class="btn btn-default") Import
